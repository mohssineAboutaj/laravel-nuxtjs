{
  "name": "laravel-nuxtjs",
  "description": "A simple Laravel & Nuxtjs starter template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt -c frontend/nuxt.config.js",
    "build": "nuxt build -c frontend/nuxt.config.js",
    "start": "nuxt start -c frontend/nuxt.config.js",
    "generate": "nuxt generate -c frontend/nuxt.config.js",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "core-js": "^3.15.1",
    "nuxt": "^2.15.7",
    "vuetify": "^2.5.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.12.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "prettier": "^2.3.2"
  },
  "author": {
    "name": "Mohssine Aboutaj",
    "email": "mohssineaboutaj1995@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "laravel",
